<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomProgressBarStyle">
        <attr name="progress_style">
            <enum name="horizonal" value="0"/>
            <enum name="circle" value="1"/>
        </attr>
        <attr name="radius" format="dimension"/>
        <attr name="begin_angle" format="float"/>
        <attr name="reach_color" format="color"/>
        <attr name="reach_height" format="dimension"/>
        <attr name="unreach_color" format="color"/>
        <attr name="unreach_height" format="dimension"/>
        <attr name="text_color" format="color"/>
        <attr name="text_size" format="dimension"/>
        <!--字体与进度条中间的间隙-->
        <attr name="text_offset" format="dimension"/>
        <attr name="begin_color" format="color"/>
        <attr name="end_color" format="color"/>

    </declare-styleable>
</resources>
